{
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "allowJs": true,
        "checkJs": false,
        "noEmit": false,
        "rootDir": ".",
        "outDir": "../../dist/server"
    },
    "include": ["./**/*"],
}
