{
    "name": "comfyuimini",
    "version": "1.5.0",
    "main": "dist/server/index.js",
    "scripts": {
        "start": "node .",
        "build:server": "tsc -p src/server/tsconfig.json",
        "build:client": "tsc -p src/client/tsconfig.json",
        "build": "npm run build:server && npm run build:client",
        "dev": "concurrently \"npm run dev:client\" \"ts-node-dev --project src/server/tsconfig.json src/server/index.ts\"",
        "dev:server": "tsc -w -p src/server/tsconfig.json",
        "dev:client": "tsc -w -p src/client/tsconfig.json",
        "dev_old": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"nodemon dist/server/index.js --watch dist/server\"",
        "format": "prettier --write ."
    },
    "keywords": [],
    "author": "ImDarkTom",
    "repository": {
        "type": "git",
        "url": "https://github.com/ImDarkTom/ComfyUIMini.git"
    },
    "engines": {
        "node": ">=15.6.0"
    },
    "license": "AGPL-3.0",
    "dependencies": {
        "@types/config": "^3.3.5",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^5.0.0",
        "@types/multer": "^1.4.12",
        "@types/winston": "^2.4.4",
        "@types/ws": "^8.5.12",
        "axios": "^1.7.2",
        "config": "^3.3.12",
        "cookie-parser": "^1.4.6",
        "ejs": "^3.1.10",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3",
        "winston": "^3.14.2",
        "ws": "^8.18.0"
    },
    "description": "",
    "devDependencies": {
        "concurrently": "^9.1.0",
        "prettier": "^3.3.3",
        "ts-node-dev": "^2.0.0"
    }
}